{
	"name": "libadwaita",
	"colors": {
		"editor.background": "#1e1e1e",
		"editor.foreground": "#fefefe",
		"activityBarBadge.background": "#26a269",
		"activityBar.background": "#303030",
		"statusBar.background": "#303030",
		"titleBar.activeBackground": "#303030",
		"titleBar.inactiveBackground": "#343434",
		"tab.inactiveBackground": "#303030",
		"editorGroupHeader.tabsBackground": "#303030",
		"sideBar.background": "#303030",
		"sideBarSectionHeader.background": "#303030",
		"menu.background": "#303030",
		"menu.selectionBackground": "#26a26877",
		"selection.background": "#26a26877",
		"editor.selectionBackground": "#26a26877",
		"list.inactiveSelectionBackground": "#1e1e1e",
		"list.activeSelectionBackground": "#26a26877",
		"list.focusOutline": "#26a268",
		"button.background": "#26a268",
		"textLink.foreground": "#26a268",
		"textLink.activeForeground": "#2ec27e",
		"list.focusHighlightForeground": "#26a268",
		"list.highlightForeground": "#26a268",
		"focusBorder": "#26a268",
		"input.background": "#343434",
		"editorSuggestWidget.background": "#303030",
		"editorWidget.background": "#303030",
		"editor.findMatchHighlightBackground": "#26a26877",
		"editor.wordHighlightBackground": "#26a26877",
		"editor.wordHighlightStrongBackground": "#ffffff39",
		"editorGutter.addedBackground": "#26a268",
		"editorGutter.modifiedBackground": "#f6d32d",
		"editorLightBulb.foreground": "#f6d32d",
	},
	"tokenColors": [
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#2ec27e"
			}
		},
		{
			"name": "Values",
			"scope": [
				"string",
				"constant.numeric",
				"constant.language.undefined",
				"constant.language"
			],
			"settings": {
				"foreground": "#f5c211"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#8ff0a4"
			}
		},
		{
			"name": "Type",
			"scope": [
				"meta.type",
				"entity.name.type",
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#33d17a"
			}
		},
		{
			"name": "Properties",
			"scope": [
				"variable.other.property",
				"meta.object-literal.key",
				"support.variable.property",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#ccc"
			}
		},
		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#2ec27e"
			}
		},
		{
			"name": "HTML attributes",
			"scope": [
				"meta.attribute",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#8ff0a4"
			}
		},
		{
			"name": "Labels",
			"scope": [
				"punctuation.definition.variable",
				"punctuation.definition.keyword",
				"punctuation.definition.entity",
				"punctuation.definition.list",
				"punctuation.definition.link",
				"punctuation.definition.metadata",
				"entity.name.label"
			],
			"settings": {
				"foreground": "#ccc"
			}
		},
		{
			"name": "CSS classes and ids",
			"scope": [
				"entity.other.attribute-name.class",
				"entity.other.attribute-name.id",
			],
			"settings": {
				"foreground": "#f5c211"
			}
		},
		{
			"name": "CSS pseudoclasses",
			"scope": [
				"entity.other.attribute-name.pseudo-class",
			],
			"settings": {
				"foreground": "#8ff0a4"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#999"
			}
		},
		{
			"name": "Markdown Heading",
			"scope": [
				"markup.heading"
			],
			"settings": {
				"foreground": "#2ec27e"
			}
		},
	]
}